/**index.wxss**/

page {
  background-color: #fff;
}

.top {
  height: 266rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
}

.top_box {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: space-around;
  /* padding: 60rpx 0 90rpx 0; */
  z-index: 1;
  color: #fff;
}

.top_item {
  /* display: flex; *//* flex-direction: column; *//* flex: 1; *//* height: 100%; *//* align-items: center; *//* justify-content: space-between; */
  font-size: 40rpx;
  font-weight: bold;
  text-align: center;
}

.top_text {
  font-size: 30rpx;
  /* margin-top: 50rpx; */
}

.top image {
  width: 100%;
}

.list {
  font-size: 34rpx;
  color: #333;
  font-weight: bold;
  padding: 30rpx 25rpx;
}

.index_item {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx 25rpx;
  border-bottom: 2rpx solid #f3f5f7;
  font-size: 26rpx;
  color: #333;
}

.index_left {
  display: flex;
  align-items: center;
  justify-content: center;
  font-weight: bold;
}

.icons_box {
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 32rpx;
  font-weight: bold;
  margin-right: 50rpx;
}

.icons {
  height: 50rpx;
  width: 50rpx;
  margin: 0 10rpx 0 0;
}
