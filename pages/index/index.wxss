/**index.wxss**/
.top{
  height: 266rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.top_box{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
 padding: 60rpx 0 90rpx 0;
  z-index: 1;
  color: #fff;
}
.top_item{
  display: flex;
  flex-direction: column;
  flex: 1;
  height: 100%;
  align-items: center;
  justify-content: space-between;
  font-size: 40rpx;
  font-weight: bold;
}
.top_text{
  font-size: 30rpx;
}
.top image{
  width: 100%
}